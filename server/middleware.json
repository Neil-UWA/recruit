{
  "initial:before": {
    "loopback#favicon": {
      "params": "$!../client/images/favicon.ico"
    }
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    },
    "morgan": {
      "params": "dev"
    }
  },
  "session": {
  },
  "auth": {
  },
  "parse": {
    "body-parser#json": {},
    "body-parser#urlencoded": {
      "params": {
        "extended": true
      }
    }
  },
  "routes": {
  },
  "files": {
    "loopback#static": [{
      "params": "$!../client"
    }, {
      "params": "$!../vendors"
    }]
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "errorhandler": {}
  }
}
